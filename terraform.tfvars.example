namespace        = "goci"
stage            = "prod"
root_domain      = "goci.io"
region           = "eu1"
vpc_state_module = "vpc/eu1.tfstate"
tf_bucket        = "goci-prod-terraform-state"
certificate_arn  = "arn:aws:acm:<region>:<account-id>:certificate/<certificate-id>"
instance_groups  = [
  {
    name          = "worker"
    instance_type = "t2.small"
  },
  {
    name          = "memory"
    instance_type = "r4.large"
  }
]
